{
  "name": "project-muse",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:ci": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.28",
    "@hookform/resolvers": "^3.9.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "^8.4.5",
    "@react-navigation/material-top-tabs": "^7.4.4",
    "@rneui/themed": "^4.0.0-rc.8",
    "@stripe/stripe-react-native": "0.50.3",
    "@supabase/supabase-js": "^2.45.1",
    "@tanstack/react-query": "^5.53.1",
    "aes-js": "^3.1.2",
    "ai": "^5.0.87",
    "cross-fetch": "^3.1.8",
    "date-fns": "^3.6.0",
    "expo": "54.0.22",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.15",
    "expo-device": "~8.0.8",
    "expo-file-system": "^19.0.17",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-media-library": "^18.2.0",
    "expo-notifications": "~0.32.11",
    "expo-router": "~6.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.52.2",
    "react-native": "0.81.4",
    "react-native-get-random-values": "^1.11.0",
    "react-native-pager-view": "^6.9.1",
    "react-native-purchases": "^9.6.8",
    "react-native-purchases-ui": "^9.6.8",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-toast-notifications": "^3.4.0",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "^0.6.1",
    "zod": "^3.25.76",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "12.3.7",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/aes-js": "^3.1.4",
    "@types/jest": "^30.0.0",
    "@types/react": "~18.2.45",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.13",
    "typescript": "^5.1.3"
  },
  "overrides": {
    "whatwg-fetch": "npm:cross-fetch@^3.1.8"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|zustand|@react-native/virtualized-lists)"
    ],
    "testMatch": [
      "**/__tests__/**/*.(test|spec).(ts|tsx|js)",
      "**/*.(test|spec).(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**",
      "!src/**/*.test.{ts,tsx}",
      "!src/**/__tests__/**"
    ]
  }
}
